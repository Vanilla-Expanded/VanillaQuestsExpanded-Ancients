    <defName>VQE_AncientResearchVault</defName>
    <root Class="VanillaQuestsExpandedAncients.QuestNode_Root_AncientResearchVault" />
    <questNameRules>
      <rulesStrings>
        <li>questName->ancient research vault</li>
      </rulesStrings>
    </questNameRules>
    <questDescriptionRules>
      <rulesStrings>
        <li>questDescription(askerIsNull==True)->text.todo</li>
      </rulesStrings>
    </questDescriptionRules>
    <modExtensions>
      <li Class="VEF.Storyteller.QuestChainExtension">
        <questChainDef>VQE_TheAncientLab</questChainDef>
        <conditionSucceedQuests>
          <li>VQE_ArchiteControlVault</li>
        </conditionSucceedQuests>
        <ticksSinceSucceed>15000</ticksSinceSucceed>
        <grantAgainOnSuccess>true</grantAgainOnSuccess>
        <daysUntilGrantAgainOnSuccess>60</daysUntilGrantAgainOnSuccess>
        <grantAgainOnFailure>true</grantAgainOnFailure>
        <daysUntilGrantAgainOnFailure>60</daysUntilGrantAgainOnFailure>
        <grantAgainOnExpiry>true</grantAgainOnExpiry>
        <daysUntilGrantAgainOnExpiry>60</daysUntilGrantAgainOnExpiry>
      </li>
    </modExtensions>
  </QuestScriptDef>
</Defs>