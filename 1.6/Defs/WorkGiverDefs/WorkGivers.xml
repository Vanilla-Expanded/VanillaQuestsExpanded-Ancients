<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <WorkGiverDef ParentName="HaulToBase">
    <defName>VQEA_HaulToArchogenInjector</defName>
    <label>haul resources to archogen injector</label>
    <giverClass>VanillaQuestsExpandedAncients.WorkGiver_HaulToArchogenInjector</giverClass>
  </WorkGiverDef>

  <WorkGiverDef>
    <defName>VQEA_EnterArchogenInjector</defName>
    <label>enter archogen injector</label>
    <giverClass>VanillaQuestsExpandedAncients.WorkGiver_EnterArchogenInjector</giverClass>
    <verb>enter</verb>
    <gerund>entering</gerund>
    <priorityInType>90</priorityInType>
    <requiredCapacities>
      <li>Moving</li>
    </requiredCapacities>
  </WorkGiverDef>

  <WorkGiverDef>
    <defName>VQEA_CarryToArchogenInjector</defName>
    <label>carry to archogen injector</label>
    <giverClass>VanillaQuestsExpandedAncients.WorkGiver_CarryToArchogenInjector</giverClass>
    <workType>Hauling</workType>
    <verb>inject</verb>
    <gerund>injecting</gerund>
    <priorityInType>90</priorityInType>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
    <canBeDoneByMechs>false</canBeDoneByMechs>
  </WorkGiverDef>

</Defs>