<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<ThingDef ParentName="VQEA_AncientsBuildingBase">
		<defName>VQEA_PneumaticTubeLaunchPort</defName>
		<label>pneumatic tube launch port</label>
		<description>A large transfer station for the ancient pneumatic tube network. It was used to send supply capsules between distant vaults, moving food, tools, and research materials through sealed underground tunnels. Cargo can still be loaded and launched, and a return capsule may arrive from another vault after some time.</description>
		<thingClass>VanillaQuestsExpandedAncients.Building_PneumaticTubeLaunchPort</thingClass>
		<tickerType>Normal</tickerType>
		<size>(5,5)</size>

		<destroyable>false</destroyable>
		<useHitPoints>false</useHitPoints>
		<passability>Impassable</passability>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<selectable>true</selectable>
		<fillPercent>1</fillPercent>
		<statBases>

			<Flammability>0</Flammability>
			<Beauty>0</Beauty>
		</statBases>
		<graphicData>
			<texPath>Things/Building/Exotic/PneumaticTubeLauncher</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(5,5)</drawSize>
		</graphicData>
		<comps>
			<li Class="VanillaQuestsExpandedAncients.CompProperties_PneumaticTransporter">
				<massCapacity>100</massCapacity>
				<max1PerGroup>True</max1PerGroup>
			</li>
		</comps>
	</ThingDef>

	<ThingDef ParentName="VQEA_AncientsBuildingBase">
		<defName>VQEA_PneumaticTubes</defName>
		<label>pneumatic tubes</label>
		<description>A network of ancient tubes used to deliver small and medium items between vaults. Some of the capsules remain stuck inside.</description>
		<size>(2,4)</size>

		<destroyable>false</destroyable>
		<useHitPoints>false</useHitPoints>
		<passability>Impassable</passability>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<selectable>true</selectable>
		<rotatable>true</rotatable>
		<fillPercent>1</fillPercent>
		<statBases>

			<Flammability>0</Flammability>
			<Beauty>0</Beauty>
		</statBases>
		<graphicData>
			<texPath>Things/Building/Exotic/PneumaticTubes</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,4)</drawSize>
		</graphicData>
	</ThingDef>

</Defs>