		<modExtensions>
			<li Class="VEF.Buildings.LootableBuildingDetails">
				<contents>
					<li>
						<thing>Silver</thing>
						<count>200</count>
					</li>
					<li>
						<thing>Pemmican</thing>
						<count>400</count>
					</li>
					<li>
						<thing>MedicineHerbal</thing>
						<count>20</count>
					</li>
					<li>
						<thing>WoodLog</thing>
						<count>500</count>
					</li>
					
				</contents>
				<randomFromContents>true</randomFromContents>
				<totalRandomLoops>1~1</totalRandomLoops>
				<buildingLeft>VQEA_PneumaticTubes_Terminus</buildingLeft>
			</li>
		</modExtensions>
