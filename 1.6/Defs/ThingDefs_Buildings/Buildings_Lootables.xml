				<gizmoDesc>VQEA_OverrideDesc</gizmoDesc>
				<cancelLootinggizmoDesc>VQEA_CancelOverrideDesc</cancelLootinggizmoDesc>
				<cancelLootinggizmoText>VQEA_CancelOverride</cancelLootinggizmoText>
				<cancelLootingGizmoTexture>UI/Designators/Cancel</cancelLootingGizmoTexture>
				<buildingLeft>VQEA_AncientPipelineJunction_Off</buildingLeft>
			</li>
		</modExtensions>
	</ThingDef>
