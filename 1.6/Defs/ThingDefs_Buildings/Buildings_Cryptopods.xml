			<neverBuildable>true</neverBuildable>
		</building>
		<modExtensions>
			<li Class="VanillaQuestsExpandedAncients.ContainmentDetails">
				<containmentSpawn>VQE_Patient</containmentSpawn>
			</li>
		</modExtensions>
	</ThingDef>


