<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <SitePartDef>
    <defName>VQEA_AncientLabComplexSite</defName>
    <label>ancient lab complex</label>
    <description>A small underground lab complex.</description>
    <siteTexture>World/WorldObjects/Sites/GenericSite</siteTexture>
    <expandingIconTexture>World/WorldObjects/Expanding/Sites/AncientLabComplex</expandingIconTexture>
    <applyFactionColorToSiteTexture>true</applyFactionColorToSiteTexture>
    <gravShipsCanLandOn>true</gravShipsCanLandOn>
    <modExtensions>
      <li Class="VanillaQuestsExpandedAncients.AncientComplexSiteExtension">
        <surfaceGenerator>VQEA_OvergroundBunkersTier1Structure</surfaceGenerator>
        <undergroundGenerator>VQEA_AncientLabComplexStructure</undergroundGenerator>
      </li>
    </modExtensions>
  </SitePartDef>

  <GenStepDef>
    <defName>VQEA_AncientLabComplex</defName>
    <linkWithSite>VQEA_AncientLabComplexSite</linkWithSite>
    <order>450</order>
    <genStep Class="VanillaQuestsExpandedAncients.GenStep_AncientComplex">
    </genStep>
  </GenStepDef>

